  "version": "2.2.1",
  "author": "Thomas Carvalho <carvalho.thomas@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomascarvalho/gatsby-plugin-mixpanel.git"
  },
