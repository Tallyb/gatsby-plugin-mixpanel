    "cross-env": "^5.2.0",
    "prettier": "^1.14.2"
  },
  "dependencies": {
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-runtime": "^6.26.0",
    "mixpanel-browser": "^2.22.4",
    "react-mixpanel": "^0.0.11"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
